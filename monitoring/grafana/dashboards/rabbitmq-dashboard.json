{
  "dashboard": {
    "id": null,
    "title": "RabbitMQ Monitoring",
    "tags": ["rabbitmq", "message-queue", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Queue Messages",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages{queue=\"notification.queue\"}",
            "legendFormat": "Main Queue"
          },
          {
            "expr": "rabbitmq_queue_messages{queue=\"notification.dlq\"}",
            "legendFormat": "Dead Letter Queue"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"drawStyle": "line", "lineWidth": 2}
          }
        }
      },
      {
        "id": 2,
        "title": "Message Rates",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(rabbitmq_queue_messages_published_total{queue=\"notification.queue\"}[5m])",
            "legendFormat": "Publish Rate"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total{queue=\"notification.queue\"}[5m])",
            "legendFormat": "Delivery Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Consumer Count",
        "type": "stat",
        "targets": [
          {
            "expr": "rabbitmq_queue_consumers{queue=\"notification.queue\"}",
            "legendFormat": "Active Consumers"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Message Processing Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rabbitmq_queue_message_processing_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(rabbitmq_queue_message_processing_seconds_bucket[5m]))",
            "legendFormat": "Median"
          }
        ],
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 8},
        "unit": "s"
      },
      {
        "id": 5,
        "title": "Queue Backlog Age",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages_ready_oldest_seconds{queue=\"notification.queue\"}",
            "legendFormat": "Oldest Message Age"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "unit": "s"
      },
      {
        "id": 6,
        "title": "Unacknowledged Messages",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages_unacknowledged{queue=\"notification.queue\"}",
            "legendFormat": "Unacknowledged"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Connection Count",
        "type": "stat",
        "targets": [
          {
            "expr": "rabbitmq_connections",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Channel Count",
        "type": "stat",
        "targets": [
          {
            "expr": "rabbitmq_channels",
            "legendFormat": "Active Channels"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 24}
      },
      {
        "id": 9,
        "title": "Node Health",
        "type": "stat",
        "targets": [
          {
            "expr": "rabbitmq_node_mem_used / rabbitmq_node_mem_limit * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 24},
        "unit": "percent"
      },
      {
        "id": 10,
        "title": "Disk Space",
        "type": "gauge",
        "targets": [
          {
            "expr": "rabbitmq_node_disk_free / rabbitmq_node_disk_free_limit * 100",
            "legendFormat": "Disk Free %"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 24},
        "unit": "percent"
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
